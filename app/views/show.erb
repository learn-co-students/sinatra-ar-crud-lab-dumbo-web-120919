
<h1>Article</h1>
<div class="article">
<p>Title: <%=@article.title%></p><br>
<p>Content: <%=@article.content%></p><br>
</div>
<form action="/articles/<%= @article.id %>" method="POST">
  <input type="hidden" name="_method" value="DELETE">
  <input type="submit" value="Delete <%= @article.id %>">
</form>